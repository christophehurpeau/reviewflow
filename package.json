{
  "name": "reviewflow",
  "private": true,
  "version": "1.88.0",
  "description": "Open Source Github Bot that improves PR flow from opening to merging.",
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "https://github.com/christophehurpeau/reviewflow.git",
  "homepage": "https://github.com/christophehurpeau/reviewflow",
  "type": "commonjs",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "packageManager": "yarn@3.1.1",
  "main": "./dist/index-node12.cjs.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "development": "./dist/index-node12-dev.cjs.js",
        "default": "./dist/index-node12.cjs.js"
      }
    }
  },
  "scripts": {
    "build": "pob-build",
    "checks": "node scripts/check-package.js",
    "clean": "rm -Rf dist",
    "generate:test-coverage": "rm -Rf docs/coverage/ ; NODE_ENV=production BABEL_ENV=test jest --coverage --coverageReporters=pob-lcov-reporter --coverageDirectory=docs/coverage/",
    "lint": "yarn run lint:prettier && tsc && yarn run lint:eslint",
    "lint:eslint": "eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet *.{js,cjs,mjs} src scripts",
    "lint:prettier": "prettier --check .",
    "postinstall": "pob-root-postinstall",
    "preversion": "yarn run lint && yarn run build && yarn run test && repository-check-dirty",
    "release": "repository-check-dirty && yarn preversion && standard-version -a -m 'chore(release): %s [skip ci]' && git push --follow-tags origin master",
    "start": "node dist/index-node10.cjs.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "commitlint": {
    "extends": [
      "@pob/commitlint-config"
    ]
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/*.test.ts?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts?(x)"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "testEnvironment": "node"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "12",
        "formats": [
          "cjs"
        ]
      }
    ],
    "jsx": true,
    "entries": [
      "index"
    ]
  },
  "dependencies": {
    "@commitlint/parse": "14.0.0",
    "@octokit/core": "3.5.1",
    "@octokit/rest": "18.12.0",
    "@octokit/webhooks": "9.17.0",
    "@slack/web-api": "6.0.0",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.6",
    "dotenv": "8.6.0",
    "emoji-regex": "10.0.0",
    "issue-parser": "6.0.0",
    "jsonwebtoken": "8.5.1",
    "liwi-mongo": "8.3.1",
    "lock": "1.1.0",
    "probot": "12.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "simple-oauth2": "4.2.0",
    "slackify-markdown": "4.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/preset-react": "7.16.0",
    "@pob/commitlint-config": "3.3.0",
    "@pob/eslint-config": "47.0.0",
    "@pob/eslint-config-typescript": "47.0.1",
    "@pob/eslint-config-typescript-react": "47.0.0",
    "@pob/root": "5.9.0",
    "@rollup/plugin-run": "2.1.0",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.13",
    "@types/issue-parser": "3.0.1",
    "@types/jest": "27.0.3",
    "@types/jsonwebtoken": "8.5.6",
    "@types/node": "14.18.0",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/simple-oauth2": "4.1.1",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "babel-jest": "27.4.2",
    "babel-preset-latest-node": "5.5.1",
    "check-package-dependencies": "2.2.1",
    "eslint": "7.32.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "38.0.1",
    "jest": "27.4.3",
    "nock": "13.2.1",
    "pob-babel": "28.6.0",
    "pob-lcov-reporter": "5.4.0",
    "prettier": "2.5.1",
    "rollup": "2.60.2",
    "smee-client": "1.2.2",
    "standard-version": "9.3.2",
    "type-fest": "2.8.0",
    "typescript": "4.5.2"
  }
}
